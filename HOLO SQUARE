#include <stdio.h>
int main() {
    int n=10;
    for(int i=0; i<n; i++)
    {
        if(i==0 || i==n-1)
        {
            for(int j=0;j<n;j++)
            {
                printf("* \t");
            }
            printf("\n");
        }
        else
        {
            for(int j=0;j<n;j++)
            {
                if(j==0 || j==n-1)  printf("* \t");
                else printf(" \t");
            }
            printf("\n");
        }
    }
    return 0;
}
