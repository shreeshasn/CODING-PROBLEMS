#include <stdio.h>
int main() {

    int n=10, a[10][10];
    for(int i=0; i<n; i++)
    {
        for(int j=0; j<n; j++)
        {
            a[i][j]=a[j][i]=j+1;
        }
    }
    
    for(int i=0; i<n; i++)
    {
        for(int j=0; j<n; j++)
        {
            printf("%d \t", a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
